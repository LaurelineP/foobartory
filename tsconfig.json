{
  "compilerOptions"		: {
	"allowJs"							: true,
	"skipLibCheck"						: true,
	"esModuleInterop"					: true,
	"strict"							: false,
	"resolveJsonModule"					: true,
	"isolatedModules"					: true,
	"noEmit"							: true,
	"allowSyntheticDefaultImports"		: true,
	"noFallthroughCasesInSwitch"		: true,
	"forceConsistentCasingInFileNames"	: true,
	"target"      		: "es5",
	"lib"				: [
		"dom",
		"dom.iterable",
		"esnext"
	],
	"module"			: "esnext",
	"moduleResolution"	: "node",
	"jsx"				: "react-jsx",
  },
  "include"				: [
	"src",
	"data.model.ts",
  ]
}
